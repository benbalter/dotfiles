{
    "editor.renderWhitespace": "boundary",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "git.enableCommitSigning": true,
    "githubPullRequests.telemetry.enabled": false,
    "markdown.extension.list.indentationSize": "inherit",
    "markdown.extension.toc.unorderedList.marker": "*",
    "ruby.codeCompletion": "rcodetools",
    "ruby.format": "rubocop",
    "ruby.intellisense": "rubyLocate",
    "window.zoomLevel": 0,
    "git.autofetch": true,
    "[yaml]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[markdown]": {
        "editor.defaultFormatter": "yzhang.markdown-all-in-one"
    },
    "cSpell.enabledLanguageIds": [
        "asciidoc",
        "c",
        "cpp",
        "csharp",
        "css",
        "git-commit",
        "handlebars",
        "html",
        "jade",
        "java",
        "javascript",
        "javascriptreact",
        "json",
        "jsonc",
        "latex",
        "less",
        "markdown",
        "php",
        "plaintext",
        "pug",
        "python",
        "restructuredtext",
        "rust",
        "scala",
        "scss",
        "text",
        "typescript",
        "typescriptreact",
        "yaml",
        "yml"
    ],
    "files.defaultLanguage": "markdown",
    "editor.tabSize": 2,
    "todo.archive.name": "Done",
    "todo.archive.sortByDate": true,
    "todo.symbols.box": "[ ]",
    "todo.symbols.cancelled": "x",
    "todo.symbols.done": "[x]",
    "markdown.todo.symbols.bullet": "*",
    "markdownShortcuts.icons.bold": false,
    "markdownShortcuts.icons.bullets": false,
    "markdownShortcuts.icons.italic": false,
    "markdownShortcuts.icons.strikethrough": false,
    "languageToolLinter.external.url": "http://localhost:7778",
    "languageToolLinter.languageTool.motherTongue": "en-US",
    "languageToolLinter.languageTool.language": "en-US",
    "workbench.sideBar.location": "left",
    "editor.fontFamily": "'Hack Nerd Font', Menlo, Monaco, 'Courier New', monospace",
    "terminal.integrated.fontFamily": "'Hack Nerd Font', Menlo, Monaco, 'Courier New', monospace"
}
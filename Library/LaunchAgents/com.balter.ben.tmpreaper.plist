<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
	<dict>
		<key>Label</key>
		<string>com.balter.ben.tmpreaper</string>
		<key>Program</key>
		<string>/usr/local/sbin/tmpreaper</string>
		<key>ProgramArguments</key>
		<array>
			<string>5d</string>
			<string>~/Downloads</string>
		</array>
		<key>StartCalendarInterval</key>
		<dict>
			<key>Hour</key>
			<integer>0</integer>
			<key>Minute</key>
			<integer>0</integer>
		</dict>
	</dict>
</plist>
